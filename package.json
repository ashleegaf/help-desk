{
  "name": "help-desk",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ashleegaf/help-desk"
  },
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "prisma generate && prisma db push && next build",
