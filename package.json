    "bcrypt": "^5.1.0",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "antd": "^5.7.3",
    "autoprefixer": "^10.4.4",
    "eslint": "8.11.0",
    "eslint-config-next": "^13.0.5",
    "postcss": "^8.4.12",
    "prisma": "^5.1.1",
    "tailwindcss": "^3.0.23",
